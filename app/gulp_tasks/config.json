{
  "folders" : {
    "src" : "./src",
    "dist" : "./dist"
  },
  "app_params" : {
    "path":"http://localhost:8887/",
    "start_url": "http://localhost:8887/index.html",
    "scope": "/",
    "mapboxkey": "_MAPBOX_API_KEY_",
    "endpoints": {
        "restaurants": "http://localhost:1337/restaurants/"
      }
  },
  "browserconfig" : "./src/browserconfig.xml",
  "manifest" : "./src/manifest.json",
  "service_worker" : "./src/ServiceWorker.js",
  "images" : {
    "root" : "./src/*.+(png|ico)",
    "assets" : "./src/img/**/*.+(png|jpg|jpeg|gif|svg|ico)"
  },
  "pages" : ["./src/index.html", "./src/restaurant.html"],
  "sw_files" : [
    "'/'",
    "'manifest.json'",
    "'browserconfig.xml'",
    "'index.html'",
    "'restaurant.html'",
    "'js/app.min.js'",
    "'js/index.min.js'",
    "'js/restaurant.min.js'",
    "'css/critical.min.css'",
    "'css/index.min.css'",
    "'css/restaurant.min.css'"],
  "css" : {
    "index" : [
      "./src/css/no_results_message.css",
      "./src/css/map.css",
      "./src/css/select_widget.css",
      "./src/css/index.css"
    ],
    "restaurant" : [
      "./src/css/no_results_message.css",
      "./src/css/map.css",
      "./src/css/restaurant.css",
      "./src/css/reviews.css"
    ],
    "critical" : ["./src/css/critical.css", "./src/css/notifications.css"]
  },
  "js" : {
    "app" : [
      "./src/js/variables.js",
      "./src/js/idb.js",
      "./src/js/datastore.js",
      "./src/js/dbhelper.js",
      "./src/js/fileLoader.js",
      "./src/js/interface_manager.js",
      "./src/js/notifications.js",
      "./src/js/app.js"
    ],
    "index" : [
      "./src/js/select_widget.js",
      "./src/js/map.js",
      "./src/js/index.js"
    ],
    "restaurant" : [
      "./src/js/map.js",
      "./src/js/restaurant.js"
    ]
  }
}
